  "compilerOptions": {
    "module": "nodenext",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "sourceMap": true,
    "baseUrl": "src",
    "outDir": "build"
  },
  "include": [
    "src"
  ]
}